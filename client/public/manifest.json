{
    "name": "TabTrackr",
    "description": "Base Level Extension",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
        "default_popup": "index.html",
        "default_icon": "tabtrackrzoomed.png"
    },
    "icons": {
        "16": "tabtrackrzoomed.png",
        "48": "tabtrackrzoomed.png",
        "128": "tabtrackr.png"
    },
    "permissions": [
        "tabs",
        "activeTab",
        "storage"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "background": {
        "service_worker": "background.js"
    }
}